{
  "name": "interceptor",
  "version": "0.3.1",
  "description": "A browser extension that mocks AJAX request at the browser level so you can run frontend without really starting a backend server.",
  "license": "MIT",
  "homepage": "https://github.com/code-mancers/interceptor#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/code-mancers/interceptor.git"
  },
  "keywords": [
    "interceptor",
    "mock-ajax-frontend",
    "mock-xhr-frontend",
    "chrome",
    "firefox-extension"
  ],
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@commitlint/travis-cli": "^7.0.0",
    "@types/chrome": "^0.0.65",
    "@types/classnames": "^2.2.3",
    "@types/clean-webpack-plugin": "^0.1.2",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.0",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^6.0.0",
    "@types/react-table": "^6.7.11",
    "@types/redux-logger": "^3.0.6",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-adapter-react-helper": "^1.2.3",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "gulp": "^3.9.1",
    "gulp-conventional-changelog": "^2.0.1",
    "gulp-git": "^2.8.0",
    "gulp-replace": "^1.0.0",
    "gulp-zip": "^4.2.0",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "lint-staged": "^7.2.0",
    "react-hot-loader": "1.3.0",
    "redux-logger": "^3.0.6",
    "run-sequence": "^2.2.1",
    "sign-addon": "^0.3.1",
    "source-map-loader": "^0.2.1",
    "ts-jest": "^22.0.2",
    "ts-loader": "^4.2.0",
    "tslint-react": "^3.5.1",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.0.11"
  },
  "engines": {
    "node": ">=6.11.5"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "globals": {
      "__DEV__": true,
      "__RCTProfileIsProfiling": false
    },
    "transform": {
      "\\.(ts|tsx|js|jsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/app/__tests__/setup.ts",
    "testRegex": "/__tests__/.*\\.test\\.(jsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testURL": "http://localhost/"
  },
  "scripts": {
    "build": "webpack -p --config webpack.config.prod.js",
    "watch": "webpack --config webpack.config.dev.js --watch --progress --colors",
    "test": "jest",
    "test:watch": "jest --watch",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -u -s",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "linters": {
      "./app/*.{js,jsx,ts,tsx}": [
        "prettier --config .prettierrc --write",
        "git add"
      ]
    },
    "ignore": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "axios": "^0.18.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.11",
    "match-sorter": "^2.2.3",
    "react": "^16.3.2",
    "react-chrome-redux": "^1.5.1",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.6",
    "react-simple-contenteditable": "^0.0.5",
    "react-table": "^6.8.6",
    "redux": "3.5.2",
    "redux-thunk": "^2.2.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "uuid": "^3.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
